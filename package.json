  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "NODE_ENV=production nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prettify": "prettier .",
    "clean": "nuxt cleanup",
