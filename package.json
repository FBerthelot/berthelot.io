    "test:static:fix": "npm run prettify -- --write && npm run lint -- --fix",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nuxt/devtools": "latest",
