  "private": true,
  "description": "My website",
  "author": "FBerthelot <florent@berthelot.io>",
  "license": "SEE LICENSE IN LICENSE.md FILE",
  "scripts": {
    "reveal": "rimraf static/reveal/ && cpr node_modules/reveal.js/ static/reveal/",
    "dev": "npm run reveal && nuxt",
    "build": "npm run reveal && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "jest",
    "storybook": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook && mv storybook-static static/storybook"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^7.3.1",
    "@nuxtjs/markdownit": "^2.0.0",
    "core-js": "^3.31.0",
    "firebase": "^10.1.0",
    "fireworks-js": "1.*.*",
    "nuxt": "^2.15.8",
    "remove": "^0.1.5",
    "reveal.js": "3.9.2",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.6",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@vue/test-utils": "^1.3.6",
    "axios": "^0.27.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.6.0",
    "cpr": "^3.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "flush-promises": "^1.0.2",
    "jest": "^27.4.4",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "prettier": "^2.8.8",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "vue-jest": "^3.0.4"
  }
